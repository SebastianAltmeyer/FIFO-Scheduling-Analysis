set terminal pdf size 5,3.75
set output 'baseConfiguration.pdf'

set bar 1.000000
set style data linespoints


set xtics nomirror out
set ytics nomirror out
set border 3

set xlabel "Task Utilization"
set ylabel "Schedulable Tasksets" 

set xrange [0:1]

plot  '../results/resBase_random_FIFO_OFFSETS_OPT' using 1:2 title "FIFO opt. offsets" lw 3, \
		'../results/resBase_random_FIFO_OFFSETS' using 1:2  title "FIFO given offsets" lw 3, \
		'../results/resBase_random_FIFO_NO' using 1:2 title "FIFO aperiodic" lw 3, \
		'../results/resBase_random_FPNS_OFFSETS_OPT' using 1:2  title "FPNS opt. offsets" lw 3, \
		'../results/resBase_random_FPNS_OFFSETS' using 1:2  title "FPNS with offsets" lw 3, \
		'../results/resBase_random_FPNS_NO' using 1:2 title "FPNS aperiodic" lw 3
