set terminal pdf size 5,3.75
set output 'oof-Random.pdf'

set bar 1.000000
set style data linespoints

set key top left

set yrange [0:1]

set xtics nomirror out
set ytics nomirror out
set border 3

set xlabel "Granularity" 
set ylabel "Weighted measure"

plot  '../results/res_random_gran_we' using 1:2 title "FPPS" lw 3, \
		'' using 1:7 title "FPNS" lw 3, \
		'' using 1:3 title "FIFO opt. offsets" lw 3, \
		'' using 1:4 title "FIFO given offsets" lw 3, \
		'' using 1:5 title "FIFO aperiodic" lw 3



